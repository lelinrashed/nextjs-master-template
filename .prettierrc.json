{
	"$schema": "https://json.schemastore.org/prettierrc",
	"printWidth": 100,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"singleAttributePerLine": false,
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindFunctions": ["clsx", "cn", "tw"]
}
